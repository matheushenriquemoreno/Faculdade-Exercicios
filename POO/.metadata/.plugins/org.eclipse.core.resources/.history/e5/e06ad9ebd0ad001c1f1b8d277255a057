package Exercicios;

import java.util.Scanner;

public class Exercicio10 {

	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		int numero;
		
		do {
		System.out.println("Digite um numero para fatoração: ");
		 numero= entrada.nextInt();
		} while(numero < 0);
		
	}	
	
	
}
