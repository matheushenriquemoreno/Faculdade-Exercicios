package Exercicios;

public class Exercicio19 {
	
	public static void main(String[] args) {
		
		
		for (int x = 1; x < 10; x++) {
			for (int y = 0 ; y < x ; y++){
				System.out.print("*");
				}
			System.out.print("\n");
		}
		
		for(int i = 10; i > 0; i--) {
			for(int y = 0; y < i; y++) {
				System.out.print("*");
			}
			System.out.print("\n");
		}

		
	}
	

}
