package Exercicios;

public class Exercicio25 {

	public static void main(String[] args) {
		
		
		
		

	}
	
	
	public static int[] retornaVetorPopulado(int tamanho) {
		int [] vetor = new int [tamanho];
		
		for(int i = 0; i < vetor.length; i ++) {
			System.out.println("Popule o vetor [" + i+1 + "]:");
		}
		return vetor;
	}

}
