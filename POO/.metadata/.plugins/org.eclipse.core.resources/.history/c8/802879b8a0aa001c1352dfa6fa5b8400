package Exercicios;

import java.util.Scanner;

public class Exercicio18 {
	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		
		for (int x = 1; x < 10; x++) {
			for (int y = 0 ; y < x ; y++){
				System.out.print("*" );
				}
			System.out.print("\n");
		}
			
		System.out.println ( );
		
		for(int i = 10; i > 0; i--) {
			for(int y = 0; y < i; y++) {
				System.out.print("*" );
			}
			System.out.print("\n");
		}
		

		
		
	}
}
